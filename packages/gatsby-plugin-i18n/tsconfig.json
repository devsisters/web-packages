{
  "compilerOptions": {
    "skipLibCheck": true,
    "outDir": "./",
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "es2019",
      "dom"
    ],
    "removeComments": false,
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "declaration": true,
    "jsx": "react"
  },
  "include": [
    "src"
  ],
  "exclude": [""]
}
