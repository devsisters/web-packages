{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["es2018", "dom", "esnext"],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "strict": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "noEmit": true,
    "composite": true,
    "incremental": true,
    "jsx": "preserve",
    "baseUrl": "./",
    "paths": {
      "~/*": ["*"]
    }
  },
  "include": ["./src/**/*", "./typings/*", "./gatsby-config.ts", "./gatsby-node.ts"]
}
