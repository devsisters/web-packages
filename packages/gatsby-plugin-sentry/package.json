  },
  "dependencies": {
    "@sentry/browser": "^5.28.0",
    "@sentry/react": "^5.28.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@types/react": "^16.9.15",
    "babel-preset-gatsby-package": "^0.8.0",
    "gatsby": "^2.18.7",
    "typescript": "^4.1.2"
