  },
  "dependencies": {
    "@sentry/browser": "^7.76.0",
    "@sentry/react": "^7.76.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@types/react": "^18.2.33",
    "babel-preset-gatsby-package": "^3.12.0",
    "gatsby": "^5.12.9",
    "typescript": "^5.2.2"
