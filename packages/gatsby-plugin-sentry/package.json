{
  "name": "@devsisters/gatsby-plugin-sentry",
  "version": "2.2.0",
  "description": "A GatsbyJS plugin to install sentry error tracking",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "prebuild": "tsc --emitDeclarationOnly",
    "build": "babel src -d . --extensions .ts,.tsx",
    "prepublishOnly": "yarn && yarn build"
  },
