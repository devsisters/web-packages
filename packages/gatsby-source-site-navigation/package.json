  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "gatsby-source-prismic": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "babel-preset-gatsby-package": "^0.8.0",
    "gatsby": "^2.28.0",
    "typescript": "^4.1.2"
  }
}
